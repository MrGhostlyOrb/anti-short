{
    "manifest_version": 3,
    "name": "Anti Short",
    "version": "1.0",
    "description": "The anti short",
    "icons": {
        "128": "icon.png"
    },
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "popup.js"
            ]
        }
    ],
    "action": {
        "default_icon": "icon.png"
    }
}

